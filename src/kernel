__kernel void pso(__constant const float *paramsf, __constant const uint *paramsi, __global const float *data, __constant const float *particle, __global float *out)
{
	// params float
	const float targetValue = paramsf[0];
	const float targetSigma = paramsf[1];
	const float minSigma = paramsf[2];
	
	// params uint
	const uint targetAhead = paramsi[0];
	const uint dataNoWindow = paramsi[1];
	
	const uint gid = get_global_id(0);
	const uint lid = get_local_id(0);

	// check boundary
	if (gid > 1 && gid < dataNoWindow)
	{
		out[gid] = lid;
		printf("> %u %u %u\n", lid, get_group_id(0), get_local_size(0));
	}
}